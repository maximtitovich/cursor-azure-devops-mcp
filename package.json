{
  "name": "cursor-azure-devops-mcp",
  "version": "1.0.0",
  "description": "MCP Server for Cursor IDE-Azure DevOps Integration",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "cursor-azure-devops-mcp": "./build/index.js",
    "cursor-azure-devops-mcp-sse": "./build/sse-server.js"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "tsc -w",
    "test-connection": "node build/test-connection.js",
    "sse-server": "node build/sse-server.js",
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755') && require('fs').chmodSync('build/sse-server.js', '755')\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cursor",
    "azure-devops",
    "mcp",
    "ide",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "azure-devops-node-api": "^12.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.23",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cursor-azure-devops-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cursor-azure-devops-mcp/issues"
  },
  "homepage": "https://github.com/yourusername/cursor-azure-devops-mcp#readme"
}
